{
  "name": "jupyterlab_geojs",
  "version": "0.1.0",
  "description": "A package for rendering GeoJS in Jupyter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],

  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "geojs": "^0.16.0",
    "jasmine": "^3.1.0",
    "@jupyter-widgets/base": "^1.1.10",
    "@jupyterlab/application": "^0.16.3",
    "@jupyterlab/docregistry": "^0.16.3",
    "@jupyterlab/notebook": "^0.16.3",
    "@jupyterlab/rendermime-interfaces": "^1.0.9",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "@types/react-dom": "^16.0.6",
    "react": "^16.4.0"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "author": "Kitware",
  "license": "BSD-3-Clause"
}
